{
    "$schema": "http://developer.yasoon.com/yasoon-module.json",
    "id": 16,
    "versionId": 35,
    "version": "0.1",
    "appNamespace": "com.yasoon.jira",
    "title": "Jira",
    "description": "Outlook Integration of Jira",
    "homepage": "http://www.jira.com",
    "author": {
        "name": "Andreas Schmidt",
        "email": "andreas.schmidt@yasoon.com"
    },
    "vendor": {
        "name": "yasoon UG",
        "email": "contact@yasoon.de",
        "url": "http://www.yasoon.com"
    },
    "logoImage": "logo.png",
    "notificationImage": "logo_round.png",
    "files": [
        "jira.js"
    ],
    "main": "jira.js",
    "oauth": [
        {
            "serviceName": "auth",
            "oauthVersion": 1,
            "clientId": "yasoonjira",
            "clientCertificate": "",
            "signatureMethod": "rsaSha1",
            "requestUrl": "$baseUrl$/plugins/servlet/oauth/request-token",
            "requestParameters": {
                "url": {
                    "baseUrl": "$param:baseUrl"
                }
            },
            "requestMethod": "post",
            "authorizationUrl": "$baseUrl$/plugins/servlet/oauth/authorize",
            "authorizationParameters": {
                "url": {
                    "baseUrl": "$param:baseUrl"
                }
            },
            "authorizationMethod": "post",
            "tokenUrl": "$baseUrl$/plugins/servlet/oauth/access-token",
            "tokenMethod": "post",
            "tokenParameters": {
                "url": {
                    "baseUrl": "$param:baseUrl"
                }
            }
        }
    ],
	predeliveredConfiguration: {
		"baseUrl": "https://jira.sovanta.com"
	}
}
